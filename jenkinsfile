pipeline {
    agent any
    stages {
        stage('compile and clean') {
            steps {
                sh 'mvn clean compile'
            }
        }
        stage('test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('Deploy') {
            steps {
                sh 'mvn package'
            }
        }
	stage('Archiving') {
            steps {
                achiveArtifacts '**/target/*.jar'
            }
        }
    }
}
